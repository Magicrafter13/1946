            <string>///Determine landscape or portrait, and adapt

var base_w = 640;
var base_h = 480;
var aspect = window_get_width() / window_get_height();
if (aspect &gt; 1)
{
    //landscape
    ww = base_h * aspect;
    hh = base_h
    display_set_gui_maximise((window_get_width() / ww), (window_get_height() / hh), 0, 0)
} else {
    //portrait
    ww = base_w;
    hh = base_w / aspect;
    display_set_gui_maximise((window_get_width() / ww), (window_get_height() / hh), 0, 0)
}
</string>
