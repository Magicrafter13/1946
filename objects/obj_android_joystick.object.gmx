            <string>/// Draw see clicks

var imax = 4;

for(var i = 0; i &lt; imax; i++)
{
    if device_mouse_check_button_pressed(i, mb_any) || device_mouse_check_button(i, mb_any)
    {
        var xx = device_mouse_x(i);
        var yy = device_mouse_y(i);
        draw_set_colour(make_colour_hsv(255*i/imax, 255,100));
        draw_text(xx-90, yy-90, string(xx) + "#" + string(yy));
        draw_circle(xx,yy,90,1);
        draw_set_colour(c_black)
    }
}
</string>
